  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "start:ui": "nx serve ui",
    "start:api": "nx serve api",
    "build:ui": "nx build ui",
    "build:ui:prod": "nx build ui --configuration=production",
    "build:api": "nx build api",
    "build:api:prod": "set NODE_ENV=production && nx build api --configuration=production",
    "test": "nx test",
    "deploy:ui": "nx deploy infrastructure --stacks BludgerUiStack --require-approval never",
    "deploy:api": "nx deploy infrastructure --stacks BludgerApiStack --require-approval never"
  },
